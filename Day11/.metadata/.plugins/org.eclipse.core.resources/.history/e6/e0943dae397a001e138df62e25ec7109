package Classwork;
import java.util.*;

public class prac4 {

	public static void main(String[] args) {
		Collection <String> c = new ArrayList<>();
		c.add("stars");
		c.add("in");
		c.add("the");
		c.add("sky");
		
		
		
		System.out.println(c.size()+" "+ c.toString());
		c.remove("in");
		System.out.println(c.size()+" "+ c.toString());
		
		Iterator <String>itr = c.iterator();
		while(itr.hasNext()) {
			String ele = itr.next();
			System.out.println(ele);
		}

	}

}
